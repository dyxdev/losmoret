{
  "name": "Los Moret",
  "displayName": "Los Moret",
  "expo": {
    "name": "Los Moret",
    "slug": "StoreOdooApp",
    "scheme": "storeodooapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fondo.png",
    "splash": {
      "image": "./assets/images/fondo.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "permissions": [
        "SCHEDULE_EXACT_ALARM",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "icon": "./assets/images/fondo.png",
      "package": "com.storeodooapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fondo.png",
        "backgroundImage": "./assets/images/fondo.png"
      },
      "splash": {
        "image": "./assets/images/fondo.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "icon": "./assets/images/fondo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.storeodooapp",
      "splash": {
        "image": "./assets/images/fondo.png",
        "tabletImage": "./assets/images/fondo.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "web": {
      "favicon": "./assets/images/fondo.png",
      "splash": {
        "image": "./assets/images/fondo.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "react-native-email-link",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.0.2"
  }
}